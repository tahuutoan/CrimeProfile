@model Project4.Models.QuanNguc
 

<form name="departmentForm" action="/" method="post">
    <div class="modal-header">
        <h4 class="modal-title">
            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span>
            </button>
            @*<span>@(Model.ID != null ? "Chi tiết quản ngục" : "Thêm mới quản ngục")</span>*@
        </h4>
    </div> 
    <div class="modal-body">
        @Html.HiddenFor(q => q.ID)
        <div class="row">
            <div class="col-md-12">
                <div class="form-group">
                    <div class="control-label col-md-2">
                        @Html.DisplayNameFor(q => q.TenQuanNguc)
                    </div>
                    <div class="col-md-10">
                        @Html.DisplayFor(q => q.TenQuanNguc, new { @class = "form-control" })
                        @*@Html.ValidationMessageFor(q => q.TenQuanNguc, "kk fail", new { @class = "text-danger" })*@
                    </div>
                </div>
                <div class="form-group">
                    <div class="control-label col-md-2">
                        @Html.DisplayNameFor(q => q.NgaySinh)
                    </div>
                    <div class="col-md-10">
                        @Html.DisplayFor(q => q.NgaySinh, new { @class = "form-control" })
                    </div>
                </div>
                <div class="form-group">
                    <div class="control-label col-md-2">
                        @Html.DisplayNameFor(q => q.QueQuan)
                    </div>
                    <div class="col-md-10">
                        @Html.DisplayFor(q => q.QueQuan, new { @class = "form-control" })
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="modal-footer">

    </div>
</form>



