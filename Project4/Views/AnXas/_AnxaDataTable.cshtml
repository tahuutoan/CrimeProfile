@model IEnumerable<Project4.Models.AnXa>
<div class="scrollmenu" id="table">
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.PhamNhanID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MucDoAnXa)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MucDoCaiTao)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.PhamNhanID)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.MucDoAnXa)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.MucDoCaiTao)
                </td>
                <td>
                    @Html.ActionLink("Chỉnh sửa", "Edit", new { id = item.ID }) |
                    @Html.ActionLink("Chi tiết", "Details", new { id = item.ID }) |
                    @Html.ActionLink("Xóa", "Delete", new { id = item.ID })
                </td>
            </tr>
        }

    </table>
</div>